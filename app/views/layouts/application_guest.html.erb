        <%= stylesheet_link_tag "wpstyles/owlcarousel/owl.carousel.min", media: 'all' %>
        <%= stylesheet_link_tag "wpstyles/owlcarousel/owl.theme.default.min", media: 'all' %>
    </head>
    <body>
        <!-- start header -->
        <%= render 'layouts/header_guest' %>
        <!-- end header -->
        <%= yield %>
        <!-- start footer -->
        <%= render 'layouts/footer_guest' %>
        <!-- end footer -->
    </body>
</html>